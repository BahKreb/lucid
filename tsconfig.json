{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "outDir": "./lib",
    "rootDir": "./src",
    "typeRoots": ["node_modules/@types/"]
  },
  "exclude": [
    "node_modules",
    "test"
  ]
}